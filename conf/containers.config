params {
    //optional
    containers_dir = null
}

process {
    withName: run_varianteer {
        container = "python:3.13"
    }

    withName: run_art {
        container = "quay.io/biocontainers/art:2016.06.05--h589041f_9"
    }

    withName: run_fastqc {
        container = "biocontainers/fastqc:v0.11.9_cv8"
    }

    withName: run_multiqc {
        container = "multiqc/multiqc:latest"
    }
}